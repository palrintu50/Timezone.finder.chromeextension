{
    "manifest_version":2,
    "name":"Timezone Finder",
    "version":"2.0.1",
    "description":"gives current timezone",
    "icons":{
        "128":"clock.png"
    },
    "browser_action":{
        "default_icon":"clock.png",
        "default_title":"Timezone finder",
        "default_popup":"popup.html"
    },
    "permissions":["activeTab"],
    "background":{
        "scripts":["main.js"]
      }
      
    }
